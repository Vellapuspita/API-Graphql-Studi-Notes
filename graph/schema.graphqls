# GraphQl schema example


type Query {
  studyNotes:[StudyNote!]!
  StudyNote(id: ID!): StudyNote
  me: User
}

type Mutation {
  register(input: RegisterInput!): AuthResponse!
  login(email: String!, password: String!): AuthResponse!
}

type User {
  id: ID!
  name: String!
  email: String!
}

type StudyNote {
  id: ID!
  title: String!
}

input RegisterInput{
  name: String!
  email: String!
  password: String!
  confirmPassword: String!
}

type AuthResponse{
  token: String!
  user: User!
}